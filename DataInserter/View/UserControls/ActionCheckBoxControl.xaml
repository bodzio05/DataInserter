<UserControl x:Class="DataInserter.View.UserControls.ActionCheckBoxControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DataInserter.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="400">
    <Grid>
        <Rectangle Stroke="Black" StrokeThickness="2"/>
        <StackPanel Margin="10 10 10 10" >
            <CheckBox Content="Add MaterialCode" x:Name="AddMaterialCodeCheckBox"/>
            <CheckBox Content="Add GroupReference" x:Name="AddGroupReferenceCheckBox" Margin="0 5 0 0"/>
        </StackPanel>
    </Grid>
</UserControl>
